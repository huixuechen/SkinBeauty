<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/nav_home">

    <fragment
        android:id="@+id/nav_home"
        android:name="com.example.skinbeauty.ui.home.HomeFragment"
        android:label="@string/menu_home"
        tools:layout="@layout/fragment_home" >
        <action
            android:id="@+id/action_nav_home_to_loginFragment"
            app:destination="@id/loginFragment" />
    </fragment>

    <fragment
        android:id="@+id/nav_gallery"
        android:name="com.example.skinbeauty.ui.gallery.GalleryFragment"
        android:label="@string/menu_gallery"
        tools:layout="@layout/fragment_gallery" >
        <action
            android:id="@+id/action_nav_gallery_to_conditionsFragment"
            app:destination="@id/conditionsFragment" />
    </fragment>

    <fragment
        android:id="@+id/nav_slideshow"
        android:name="com.example.skinbeauty.ui.tips.TipsFragment"
        android:label="@string/menu_slideshow"
        tools:layout="@layout/fragment_tips" >
        <action
            android:id="@+id/action_nav_slideshow_to_drySkinFragment"
            app:destination="@id/drySkinFragment" />
        <action
            android:id="@+id/action_nav_slideshow_to_wrinklesFragment"
            app:destination="@id/wrinklesFragment" />
        <action
            android:id="@+id/action_nav_slideshow_to_underEyeBagsFragment"
            app:destination="@id/underEyeBagsFragment" />
        <action
            android:id="@+id/action_nav_slideshow_to_radianceFragment"
            app:destination="@id/radianceFragment" />
        <action
            android:id="@+id/action_nav_slideshow_to_pigmentationFragment"
            app:destination="@id/pigmentationFragment" />
        <action
            android:id="@+id/action_nav_slideshow_to_firmnessFragment"
            app:destination="@id/firmnessFragment" />
        <action
            android:id="@+id/action_nav_slideshow_to_fineLinesFragment"
            app:destination="@id/fineLinesFragment" />

    </fragment>

    <fragment
        android:id="@+id/nav_profile"
        android:name="com.example.skinbeauty.ui.profile.ProfileFragment"
        android:label="@string/menu_profile"
        tools:layout="@layout/fragment_profile" >
        <action
            android:id="@+id/action_nav_profile_to_nav_home"
            app:destination="@id/nav_home" />
    </fragment>

    <fragment
        android:id="@+id/question1Fragment"
        android:name="com.example.skinbeauty.ui.questions.Question1Fragment"
        android:label="Question1"
        tools:layout="@layout/fragment_question1" >
        <action
            android:id="@+id/action_question1Fragment_to_question2Fragment"
            app:destination="@id/question2Fragment" />
    </fragment>
    <fragment
        android:id="@+id/question2Fragment"
        android:name="com.example.skinbeauty.ui.questions.Question2Fragment"
        android:label="Question2"
